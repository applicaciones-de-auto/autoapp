                                                    </graphic>
                                                </TableColumn>
                                                <TableColumn fx:id="tblindex03_part" editable="false" maxWidth="200.0" minWidth="200.0" prefWidth="200.0" resizable="false" sortable="false" text="(Sales) Accessories Description" />
