                    </StackPane.margin>
                </BorderPane>
            </children>
        </StackPane>
    </children>
    <stylesheets>
        <URL value="@../../css/FrameStyle.css" />
        <URL value="@../../css/MainPanelStyle.css" />
    </stylesheets>
